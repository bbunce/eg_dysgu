{
  "name": "eg_dysgu",
  "title": "Dysgu SV caller",
  "summary": "Paired end and long read SV caller",
  "dxapi": "1.0.0",
  "version": "0.0.1",
  "inputSpec": [
    {
      "name": "bams",
      "label": "Bam and index",
      "class": "array:file",
      "optional": false,
      "patterns": [
        "*"
      ],
      "help": ""
    },
    {
      "name": "family_number",
      "class": "string",
      "optional": false,
      "help": ""
    },
    {
      "name": "sample_number",
      "class": "string",
      "optional": false,
      "help": ""
    }
  ],
  "outputSpec": [
    {
      "name": "results",
      "class": "array:file",
      "patterns": [
        "*"
      ],
      "help": ""
    }
  ],
  "runSpec": {
    "timeoutPolicy": {
      "*": {
        "hours": 2
      }
    },
    "interpreter": "python3",
    "file": "src/eg_dysgu.py",
    "distribution": "Ubuntu",
    "release": "20.04",
    "version": "0"
  },
  "access": {
    "project": "CONTRIBUTE",
    "allProjects": "CONTRIBUTE",
    "network": [
      "*"
    ]
  },
  "regionalOptions": {
    "aws:eu-central-1": {
      "systemRequirements": {
        "*": {
          "instanceType": "mem1_ssd1_v2_x8"
        }
      }
    }
  }
}
